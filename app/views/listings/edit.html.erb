<br>
<%if current_user.id==@listing.user_id || current_user.superadmin? %>
<%="Current User ID = #{current_user.id}"%><br>
<%="Listing's User ID = #{@listing.user_id}"%><br>
<%=" Permitted"%>
<br>
<%=form_for [current_user,@listing] do |f|%>
<p>
<%=f.label :address %><br>
<%=f.text_area :address %>
</p>
<p>
<%=f.label :price_per_night %><br>
<%=f.text_field :price_per_night%>
</p>
<p>
<%=f.label :tags%><br>
<%=f.text_field :tags%>
</p>
<p>
    <%= f.file_field :photos, multiple: true%>
  </p>
<p>
<%=f.submit%>	
</p>
<%end%>
<%else%>
<%=" NOT Permitted"%>
<%end%>
